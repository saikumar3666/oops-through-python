class employee1:
    def name(self):
        print("sai is his name")
    def salary(self):
        print("3000 is his salary")
    def age(self):
        print("20 is his age")

class employee2:
    def name(self):
        print("siva is his name")
    def salary(self):
        print("3000 is his salary")
    def age(self):
        print("20 is his age")

def func(obj):
    obj.name()
    obj.salary()
    obj.age()

obj_emp = employee1()
func(obj_emp)

obj_emp2 = employee2()
func(obj_emp2) 
